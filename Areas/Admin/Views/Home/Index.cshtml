@model IQueryable<EntityBase>
@{
    string strTitle = "Панель адміністратора";
    ViewBag.Title = strTitle;
}

<div>
    <h2>@strTitle</h2><br />
    <div>
        <h3>Керування записами</h3>
        <div class="div-box">
            @Html.ActionLink("Додати запис", "Index", "Records", new { area = "Admin" }, null)
        </div>
    </div>
    <div class="div-box">
        <form asp-area="" asp-controller="Account" asp-action="Logout" method="post">
            <input type="submit" value="Вийти" />
        </form>
    </div>
</div>
